{
  "name": "@gw2/markup-unified",
  "version": "1.0.0",
  "description": "Unified processor for Guild Wars 2 Markup",
  "keywords": ["gw2", "guild wars 2", "markup", "tooltip", "ast", "unified"],
  "type": "module",
  "exports": {
    "./parse": {
      "types": "./dist/parse.d.ts",
      "default": "./dist/parse.js"
    },
    "./rehype": {
      "types": "./dist/rehype.d.ts",
      "default": "./dist/rehype.js"
    },
    "./stringify": {
      "types": "./dist/stringify.d.ts",
      "default": "./dist/stringify.js"
    },
    "./strip": {
      "types": "./dist/strip.d.ts",
      "default": "./dist/strip.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/parse.ts src/rehype.ts src/stringify.ts src/strip.ts"
  },
  "author": "darthmaim",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GW2Treasures/gw2-markup.git",
    "directory": "packages/unified"
  },
  "bugs": {
    "url": "https://github.com/gw2treasures/gw2-markup/issues"
  },
  "dependencies": {
    "@gw2/markup-ast": "workspace:^",
    "@gw2/markup-parser": "workspace:^",
    "@gw2/markup-to-hast": "workspace:^",
    "@gw2/markup-to-text": "workspace:^",
    "@gw2/markup-stringify": "workspace:^",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "tsdown": "0.20.3",
    "typescript": "5.9.3"
  }
}
