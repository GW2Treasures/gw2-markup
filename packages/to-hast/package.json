{
  "name": "@gw2/markup-to-hast",
  "version": "1.0.0",
  "description": "Convert Guild Wars 2 Markup AST to HAST",
  "keywords": ["gw2", "guild wars 2", "markup", "tooltip", "ast", "hast", "html"],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/index.ts --minify"
  },
  "author": "darthmaim",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GW2Treasures/gw2-markup.git",
    "directory": "packages/to-hast"
  },
  "bugs": {
    "url": "https://github.com/gw2treasures/gw2-markup/issues"
  },
  "dependencies": {
    "@gw2/markup-ast": "workspace:*",
    "@types/hast": "^3.0.0"
  },
  "devDependencies": {
    "tsdown": "0.20.3",
    "typescript": "5.9.3"
  }
}
