{
  "name": "@gw2/markup-strip",
  "version": "1.0.0",
  "description": "Strip Guild Wars 2 markup from strings",
  "keywords": ["gw2", "guild wars 2", "markup", "tooltip", "strip", "plain", "text"],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/strip.d.ts",
      "default": "./dist/strip.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/strip.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "author": "darthmaim",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GW2Treasures/gw2-markup.git",
    "directory": "packages/strip"
  },
  "bugs": {
    "url": "https://github.com/gw2treasures/gw2-markup/issues"
  },
  "dependencies": {
    "@gw2/markup-parser": "workspace:*",
    "@gw2/markup-to-text": "workspace:*"
  },
  "devDependencies": {
    "tsdown": "0.20.3",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  }
}
