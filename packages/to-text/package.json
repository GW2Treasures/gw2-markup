{
  "name": "@gw2/markup-to-text",
  "version": "1.0.0",
  "description": "Convert Guild Wars 2 Markup AST to plain text",
  "keywords": ["gw2", "guild wars 2", "markup", "tooltip", "ast", "text", "plain"],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/to-text.d.mts",
      "default": "./dist/to-text.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown src/to-text.ts --minify",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "author": "darthmaim",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GW2Treasures/gw2-markup.git",
    "directory": "packages/to-text"
  },
  "bugs": {
    "url": "https://github.com/gw2treasures/gw2-markup/issues"
  },
  "dependencies": {
    "@gw2/markup-ast": "workspace:*"
  },
  "devDependencies": {
    "@gw2/markup-parser": "workspace:*",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
