{
  "name": "@gw2/markup-to-jsx",
  "version": "1.0.0",
  "description": "Convert Guild Wars 2 Markup AST to JSX",
  "keywords": ["gw2", "guild wars 2", "markup", "tooltip", "ast", "jsx"],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/to-jsx.d.ts",
      "default": "./dist/to-jsx.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/to-jsx.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "author": "darthmaim",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GW2Treasures/gw2-markup.git",
    "directory": "packages/to-jsx"
  },
  "bugs": {
    "url": "https://github.com/gw2treasures/gw2-markup/issues"
  },
  "dependencies": {
    "@gw2/markup-ast": "workspace:^"
  },
  "devDependencies": {
    "@gw2/markup-parser": "workspace:*",
    "tsdown": "0.20.3",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  }
}
