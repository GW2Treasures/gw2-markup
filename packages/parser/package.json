{
  "name": "@gw2/markup-parser",
  "version": "1.0.0",
  "description": "Guild Wars 2 markup parser",
  "keywords": ["gw2", "guild wars 2", "markup", "tooltip", "parser", "ast"],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/parser.d.mts",
      "default": "./dist/parser.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/parser.ts --minify",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "author": "darthmaim",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GW2Treasures/gw2-markup.git",
    "directory": "packages/parser"
  },
  "bugs": {
    "url": "https://github.com/gw2treasures/gw2-markup/issues"
  },
  "dependencies": {
    "@gw2/markup-ast": "workspace:*"
  },
  "devDependencies": {
    "tsdown": "0.20.3",
    "typescript": "5.9.3",
    "unist-util-inspect": "8.1.0",
    "vitest": "4.0.18"
  }
}
